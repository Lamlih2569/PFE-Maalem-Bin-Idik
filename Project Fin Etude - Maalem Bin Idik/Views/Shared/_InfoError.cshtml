@if (Session["Erreur"] != null)
{
<div style="position:absolute;z-index:100;" class="alert alert-danger alert-dismissible fade show" role="alert">
    <strong>Information Incorrecte !</strong> Veuiller reverifier vos données.
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
}